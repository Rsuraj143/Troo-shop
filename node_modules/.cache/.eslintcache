nter mb-5\">\r\n          <button className=\"btn btn-outline-dark me-2\" onClick={()=>setFilter(data)}>ALL</button>\r\n          <button className=\"btn btn-outline-dark me-2\" onClick={()=>filterProduct(\"men's clothing\")}>Men's Clothing</button>\r\n          <button className=\"btn btn-outline-dark me-2\" onClick={()=>filterProduct(\"women's clothing\")}>\r\n            Women's Clothing\r\n          </button>\r\n          <button className=\"btn btn-outline-dark me-2\" onClick={()=>filterProduct(\"jewelery\")}>Jewelery</button>\r\n          <button className=\"btn btn-outline-dark me-2\" onClick={()=>filterProduct(\"electronics\")}>Electronic</button>\r\n        </div>\r\n        {filter.map((product) => {\r\n          return (\r\n            <>\r\n              <div className=\"col-md-3 mb-4 \">\r\n                <div className=\"card h-100 text-center p4\" key={product.id}>\r\n                  <img\r\n                    src={product.image}\r\n                    className=\"card-img-top\"\r\n                    alt={product.title}\r\n                    height=\"250px\"\r\n                  />\r\n                  <div className=\"card-body\" >\r\n                    <h5 className=\"card-title mb-0\">\r\n                      {product.title.substring(0, 12)}...\r\n                    </h5>\r\n                    <p className=\"card-text lead fw-bold\">${product.price}</p>\r\n                    <Link to={`/products/${product.id}`} className=\"btn btn-primary\">\r\n                      Buy Now\r\n                    </Link>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </>\r\n          );\r\n        })}\r\n      </>\r\n    );\r\n  };\r\n\r\n  return (\r\n    <div>\r\n      <div className=\"container my-5 py-5\">\r\n        <div className=\"row\">\r\n          <div className=\"col-12 mb-5\">\r\n            <h1 className=\"display-6 fw-bolder text-center\">Latest Product</h1>\r\n            <hr />\r\n          </div>\r\n        </div>\r\n        <div className=\"row justify-content-center\">\r\n          {loading ? <Loading /> : <ShowProducts />}\r\n        </div>\r\n      </div>\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default Products;\r\n","D:\\Projects\\Troo-shop\\src\\redux\\store.js",[],[],"D:\\Projects\\Troo-shop\\src\\redux\\reducer\\index.js",[],[],"D:\\Projects\\Troo-shop\\src\\redux\\reducer\\handleCart.js",["68","69"],[],"\r\n\r\nconst cart = []\r\n\r\nconst handleCart = (state=cart, action) =>{\r\n    const product = action.payload;\r\n    switch(action.type){\r\n        case \"ADDITEM\" :\r\n            const exist = state.find((x)=>x.id ===product.id)\r\n            if(exist){\r\n                return state.map((x)=>\r\n                x.id === product.id ? {...x, qty:x.qty +1} : x\r\n                );\r\n            }\r\n            else{\r\n                const product = action.payload\r\n                return[\r\n                    ...state,\r\n                    {\r\n                        ...product,\r\n                        qty:1\r\n                    }\r\n                ]\r\n            }\r\n            break;\r\n\r\n        case \"DELITEM\" :\r\n            const exist1 = state.find((x)=>x.id === product.id)\r\n            if(exist1.qty===1){\r\n                return state.filter((x)=> x.id !== exist1.id)\r\n            }\r\n            else{\r\n                state.map((x)=>\r\n                x.id === product.id ? {...x, qty:x.qty-1} : x\r\n                )\r\n            }\r\n            break;\r\n        \r\n\r\n        default:\r\n            return state;\r\n            break;\r\n    }\r\n}\r\nexport default handleCart;","D:\\Projects\\Troo-shop\\src\\redux\\action\\index.js",[],[],{"ruleId":"70","severity":1,"message":"71","line":27,"column":7,"nodeType":"72","endLine":27,"endColumn":9,"suggestions":"73"},{"ruleId":"70","severity":1,"message":"74","line":25,"column":28,"nodeType":"75","endLine":25,"endColumn":33},{"ruleId":"76","